{% if menu %}
	<ul {% if menuLocation %} id="{{ menuLocation }}NavbarList" {% endif %}>
		{% for item in items %}
		<li class="{{ item.classes | join(' ') }} {% if menuLocation %}{{ menuLocation }}NavbarListItem{% endif %}">
			<div class="navItemBorder"></div>
			<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }} {% if item.title == "Sklep" %} {% endif %}</a>
{#			{% include "menu.twig" with {'items': item.children} %}#}
{#			{% if item.title != "Sklep" %}  {% endif %}#}
			<div class="navItemBorder"></div>
		</li>
	{% endfor %}
	</ul>
{% endif %}
